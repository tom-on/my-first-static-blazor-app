{
  "routes": [
    {
      "route": "/profile",
      "allowedRoles": [ "authenticated" ]
    },
    {
      "route": "/admin/*",
      "allowedRoles": [ "administrator" ]
    }, 
    {
      "route": "/api/*",
      "methods": [ "PUT", "POST", "PATCH", "DELETE" ],
      "allowedRoles": [ "contributor" ]
    }
  ],

  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/login"
    }
  },

  "navigationFallback": {
    "rewrite": "/index.html"
  }
}